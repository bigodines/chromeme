{
  "name": "ChroMeme",
  "version": "1.0",
  "description": "ChroMeme - using Chrome to add awesome content into meme",
  "background_page" : "background_page.html",
  "browser_action": {
    "default_icon": "icon.png"
    },	
  "content_scripts": [
    { "matches": ["http://*/*"], 
      "js": ["smart_meme.js"],
      "css": ["content_class.css"] }
  ],
  "permissions": [
    "tabs", "http://*/", "https://*/" 
  ]
}